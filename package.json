    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/router-store": "1.2.5",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.2.3",
    "@ngrx/store-log-monitor": "3.0.2",
    "@types/core-js": "0.9.33",
    "@types/express": "4.0.35",
