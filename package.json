    "html-webpack-plugin": "2.26.0",
    "ip": "1.1.3",
    "istanbul-instrumenter-loader": "1.2.0",
    "json-loader": "0.5.4",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.2",
